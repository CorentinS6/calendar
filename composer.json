{
	"name": "texxasrulez/calendar",
	"type": "composer-plugin",
	"description": "CalDav Capable Calendar plugin for Roundcube and Nextcloud",
	"homepage": "https://github.com/texxasrulez/roundcube_caldav_calendar",
	"license": "GPL-3.0+",
	"version": "1.0",
	"authors": [
		{
			"name": "Gene Hawkins",
			"email": "texxasrulez@yahoo.com",
			"homepage": "https://www.genesworld.net",
			"role": "Developer"
		},
		{
			"name": "Thomas Bruederli",
			"email": "bruederli@kolabsys.com",
			"role": "Lead"
		},
		{
			"name": "Aleksander Machniak",
			"email": "machniak@kolabsys.com",
			"role": "Developer"
		}
	],
	"repositories": [
		{
            "type": "composer",
            "url": "https://plugins.roundcube.net"
		}
	],
	"funding": [
		{
			"type": "patreon",
			"url": "https://paypal.me/texxasrulez"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"league/oauth2-server": "*",
		"league/oauth2-client": "*",
		"league/oauth2-google": "*",
		"texxasrulez/libcalendaring": "*",
		"texxasrulez/libkolab": "*",
		"sabre/dav": "~4.1.0",
		"sabre/vobject": "*",
		"sabre/event": "*",
		"sabre/xml": "*",
		"sabre/http": "*",
		"sabre/uri": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8.36 || ^5.7.21",
		"evert/phpdoc-md": "~0.1.0",
		"monolog/monolog": "^1.18"
	},
	"suggest": {
		"roundcube/carddav": "v4.x-dev"
	},
	"autoload": {
		"psr-4": {
			"Sabre\\CalDAV\\": "lib/CalDAV/",
			"Sabre\\DAV\\Client\\": "lib/",
			"Sabre\\DAV\\": "lib/DAV/",
			"Sabre\\DAV\\Property\\": "lib/DAV/Xml/Property/",
			"Sabre\\DAVACL\\": "lib/DAVACL/",
			"Sabre\\HTTP\\": "lib/",
			"Sabre\\HTTP\\ClientHttpException\\": "lib/",
			"League\\OAuth2\\Server\\": "src/",
			"League\\OAuth2\\Client\\": "src/",
			"League\\OAuth2\\Google\\": "src/"
		}
	}
}